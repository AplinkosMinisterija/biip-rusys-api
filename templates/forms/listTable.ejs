<table class="w-full table-striped my-4" cellspacing="0" cellpadding="0">
  <thead class="text-left">
    <tr>
      <th class="px-4 py-2">Nr.</th>
      <th class="px-4 py-2">Pavadinimas</th>
      <th class="px-4 py-2">Lot. pavadinimas</th>
      <th class="px-4 py-2">Plotas</th>
      <th class="px-4 py-2">StebÄ—jimo data</th>
    </tr>
  </thead>
  <% if (informationalForms.length) { %>
  <tbody>
    <% informationalForms.forEach(function(form, index) { %>
    <tr>
      <td class="px-4 py-2"><%= index + 1 %>.</td>
      <td class="px-4 py-2"><%= speciesById[form.species].speciesName %></td>
      <td class="px-4 py-2 font-italic"><%= speciesById[form.species].speciesNameLatin %></td>
      <td class="px-4 py-2"><%= form.areaText %></td>
      <td class="px-4 py-2"><%= form.observedAt %></td>
    </tr>
    <% }); %>
  </tbody>
  <% } %>
</table>
